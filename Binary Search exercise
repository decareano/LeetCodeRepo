/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number}
 */
var search = function(nums, target) {
    
    //for (let i = 0; i < nums.length; i++) {
    let index = 0
    while (index < nums.length) {
        if (nums[index] === target) 
            return index
            index++
    }
    return -1
    
};
